{
  "name": "typed-sse",
  "version": "0.1.8",
  "description": "Typed helpers for Server-Sent Events (EventSource) with JSON parsing and optional reconnect.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "bun run clean && bun run build",
    "lint": "eslint . --ext .ts",
    "test": "vitest run",
    "version-patch": "npm version patch"
  },
  "keywords": [
    "sse",
    "eventsource",
    "typescript",
    "json",
    "nextjs"
  ],
  "author": "MrRise",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rising-Corporation/typed-sse.git"
  },
  "homepage": "https://github.com/Rising-Corporation/typed-sse#readme",
  "bugs": {
    "url": "https://github.com/Rising-Corporation/typed-sse/issues"
  },
  "engines": {
    "node": ">=20"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.40.0",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {}
}
