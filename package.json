{
  "name": "typed-sse",
  "version": "0.1.3",
  "description": "Typed helpers for Server-Sent Events (EventSource) with JSON parsing and optional reconnect.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "eslint . --ext .ts",
    "test": "vitest run"
  },
  "keywords": [
    "sse",
    "eventsource",
    "typescript",
    "json",
    "nextjs"
  ],
  "author": "MrRise",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Rising-Corporation/typed-sse.git"
  },
  "homepage": "https://github.com/Rising-Corporation/typed-sse#readme",
  "bugs": {
    "url": "https://github.com/Rising-Corporation/typed-sse/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "typescript": "^5.9.2",
    "@typescript-eslint/eslint-plugin": "^8.8.1",
    "@typescript-eslint/parser": "^8.8.1",
    "eslint": "^8.57.0",
    "vitest": "^2.1.3"
  },
  "peerDependencies": {}
}
