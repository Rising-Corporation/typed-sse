{
  "name": "typed-sse",
  "version": "0.1.3",
  "description": "Typed helpers for Server-Sent Events (EventSource) with JSON parsing and optional reconnect.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "sse",
    "eventsource",
    "typescript",
    "json",
    "nextjs"
  ],
  "author": "MrRise",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "devDependencies": {
    "typescript": "^5.9.2"
  },
  "peerDependencies": {}
}
